diff --git a/certificate-pdf-service/Dockerfile b/certificate-pdf-service/Dockerfile
deleted file mode 100644
--- a/certificate-pdf-service/Dockerfile	(revision f1dd326364452454a651ec6ddebeb1f755f7674a)
+++ /dev/null	(revision f1dd326364452454a651ec6ddebeb1f755f7674a)
@@ -1,29 +0,0 @@
-############################
-# STEP 1 build executable binary
-############################
-FROM golang:alpine AS builder
-RUN apk update && apk add --no-cache 'git=~2'
-ENV GO111MODULE=on
-WORKDIR $GOPATH/src/packages/certificate-pdf/
-COPY . .
-RUN go get -d -v
-RUN CGO_ENABLED=0 GOOS=linux go build -a -installsuffix cgo -o /go/main .
-
-############################
-# STEP 2 build a small image
-############################
-FROM alpine:3
-
-WORKDIR /
-
-# Copy our static executable.
-COPY --from=builder /go/main /go/main
-COPY fonts /go/fonts
-ENV PORT 8003
-ENV GIN_MODE release
-EXPOSE 8003
-
-WORKDIR /go
-
-# Run the Go Gin binary.
-ENTRYPOINT ["/go/main"]
